<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>s1</servlet-name>
        <servlet-class>First</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s2</servlet-name>
        <servlet-class>FirstGeneric</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s3</servlet-name>
        <servlet-class>DemoServlet</servlet-class>
        <init-param>
            <param-name>driver</param-name>
            <param-value>sun.jdbc.odbc.JdbcOdbcDriver</param-value>
        </init-param>
        <init-param>
            <param-name>username</param-name>
            <param-value>system</param-value>
        </init-param>
        <init-param>
            <param-name>password</param-name>
            <param-value>oracle</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>s4</servlet-name>
        <servlet-class>DemoServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s5</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s6</servlet-name>
        <servlet-class>WelcomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s7</servlet-name>
        <servlet-class>GoogleSearcher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s8</servlet-name>
        <servlet-class>DemoServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s9</servlet-name>
        <servlet-class>DemoServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s10</servlet-name>
        <servlet-class>FirstServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s11</servlet-name>
        <servlet-class>Second</servlet-class>
    </servlet>
    <servlet>
        <display-name>LoginServlet</display-name>
        <servlet-name>s12</servlet-name>
        <servlet-class>cookie.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>LogoutServlet</display-name>
        <servlet-name>s13</servlet-name>
        <servlet-class>cookie.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
    <display-name>ProfileServlet</display-name>
        <servlet-name>s14</servlet-name>
        <servlet-class>cookie.ProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s15</servlet-name>
        <servlet-class>hidden.FirstServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s16</servlet-name>
        <servlet-class>hidden.SecondServlet</servlet-class>
    </servlet>

    <context-param>
        <param-name>drivername</param-name>
        <param-value>sun.jdbc.odbc.JdbcOdbcDriver</param-value>
    </context-param>

    <context-param>
        <param-name>username</param-name>
        <param-value>system</param-value>
    </context-param>

    <context-param>
        <param-name>password</param-name>
        <param-value>oracle</param-value>
    </context-param>

    <servlet-mapping>
        <servlet-name>s1</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s2</servlet-name>
        <url-pattern>/hellogeneric</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s3</servlet-name>
        <url-pattern>/demoservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s4</servlet-name>
        <url-pattern>/demoserv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s5</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s6</servlet-name>
        <url-pattern>/welcome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s7</servlet-name>
        <url-pattern>/google</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s8</servlet-name>
        <url-pattern>/servlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s9</servlet-name>
        <url-pattern>/servlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s10</servlet-name>
        <url-pattern>/first</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s11</servlet-name>
        <url-pattern>/second</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s12</servlet-name>
        <url-pattern>/cookie/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s13</servlet-name>
        <url-pattern>/cookie/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s14</servlet-name>
        <url-pattern>/cookie/ProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s15</servlet-name>
        <url-pattern>/hidden/hiddenServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s16</servlet-name>
        <url-pattern>/hidden/hiddenServlet2</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>